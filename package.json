{
  "name": "react-emoji",
  "version": "0.5.0",
  "description": "An emoji mixin for React",
  "main": "dist/react-emoji.js",
  "scripts": {
    "start": "node example/server.js",
    "build": "rm -rf dist/* && webpack -p --config webpack.production.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --cache --ext .js --ext .jsx src test",
    "fixlint": "eslint --fix --ext .js --ext .jsx src test",
    "fmt": "prettier"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setupTestFramework",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/banyan/react-emoji.git"
  },
  "keywords": [
    "react",
    "emoji",
    "react-component"
  ],
  "author": "Kohei Hasegawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/banyan/react-emoji/issues"
  },
  "homepage": "https://github.com/banyan/react-emoji",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.12.2",
    "jest": "^19.0.2",
    "phantomjs": "^1.9.16",
    "prettier": "^0.22.0",
    "react": "~0.14.1",
    "react-addons-test-utils": "~0.14.1",
    "react-dom": "~0.14.1",
    "react-hot-loader": "^1.2.3",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "emoji-annotation-to-unicode": "^0.3.0",
    "emoji-emoticon-to-unicode": "^0.2.2",
    "escape-string-regexp": "^1.0.3",
    "lodash.compact": "^3.0.0"
  },
  "files": [
    "package.json",
    "dist/react-emoji.js"
  ]
}
